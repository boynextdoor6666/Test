{
  "name": "tez-jumush-backend",
  "version": "1.0.0",
  "description": "Backend для Tez Jumush - платформы поиска подработки (Express + TypeScript + MySQL + JWT)",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "ts-node src/migrate.ts",
    "migrate:prod": "node dist/migrate.js",
    "test": "echo \"No tests yet\" && exit 0",
    "lint": "echo \"No linting configured\" && exit 0",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "jobs",
    "work",
    "employment",
    "kyrgyzstan",
    "backend",
    "api"
  ],
  "author": "Tez Jumush Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "terser": "^5.16.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
